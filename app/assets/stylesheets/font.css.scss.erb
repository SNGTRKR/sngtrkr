@font-face {
    font-family: 'MuseoSans-100';
    src: url('<%= asset_path "museosans-100.eot" %>');
    src: url('<%= asset_path "museosans-100.eot?#iefix" %>') format('embedded-opentype'),
         url('<%= asset_path "museosans-100.woff" %>') format('woff'),
         url('<%= asset_path "museosans-100.ttf" %>') format('truetype'),
         url('<%= asset_path "museosans-100.svg#MuseoSans-100" %>') format('svg');
         font-weight: normal;
         font-style: normal;
}
