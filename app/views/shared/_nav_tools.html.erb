<% if !user_signed_in? %>
<% else %>
<div class="outer-tools">
<%= link_to(destroy_user_session_path, :method => :delete, :class => "btn btn-navbar toolbutton tooltip-parent", "data-title" => "Logout", "data-placement" => "bottom" ) do %><i class="icon-signout"></i><% end %>
<%= link_to edit_user_path(@user) do %><div class="btn btn-navbar toolbutton tooltip-parent" data-title="Settings" data-placement="bottom"><i class="icon-cog"></i></div><% end %>
<a href="#feedback" data-toggle="modal"><div class="btn btn-navbar toolbutton tooltip-parent" data-title="Feedback" data-placement="bottom"><i class="icon-comment"></i></div></a></div><% end %>
<script type="text/javascript">
$(document).ready(function() {
  $('.facebook-login').click(function(){
    $('.facebook-login > span').html('Logging in...');
    $(this).addClass('fb-disabled');
  });
});
</script>