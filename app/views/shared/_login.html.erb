<% if(user_signed_in?) %>
<div class="logout-button"><div class="o-button norm-o-pad right signika-font font-logout"><%= link_to('Log Out', destroy_user_session_path, :method => :delete) %></div></div>
<a href="/users/me"><div class="p-head right o-button"><%= image_tag "/images/profile.png", :alt => "Profile", :id => "profile-pad" %></div></a>
<% elsif %>
<%= link_to (image_tag '/images/log-facebook.png'), user_omniauth_authorize_path(:facebook), :class => 'right fb-login-nudge', :alt => "Login" %>
<% end %>
