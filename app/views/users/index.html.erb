<h1>Listing users</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Password</th>
    <th>Type</th>
    <th>Email</th>
    <th>Email frequency</th>
    <th>Last email</th>
    <th>Username</th>
    <th>Role</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.password %></td>
    <td><%= user.type %></td>
    <td><%= user.email %></td>
    <td><%= user.email_frequency %></td>
    <td><%= user.last_email %></td>
    <td><%= user.username %></td>
    <td><%= user.role %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />