<p id="notice">
	<%= notice %>
</p>

<p>
	<b>Name:</b>
	<%= @release.name %>
</p>
<p>
  <%= image_tag @release.image :medium %>
</p>
<p>
	<b>Date:</b>
	<%= @release.date %>
</p>

<p>
	<b>Cat no:</b>
	<%= @release.cat_no %>
</p>

<p>
	<b>Itunes:</b>
	<%= @release.itunes %>
</p>

<p>
	<b>Sdigital:</b>
	<%= @release.sdigital %>
</p>

<p>
	<b>Amazon:</b>
	<%= @release.amazon %>
</p>

<p>
	<b>Youtube:</b>
	<%= @release.youtube %>
</p>

<p>
	<b>Type:</b>
	<%= @release.rls_type %>
</p>

<p>
	<b>Artist:</b>
	<%= @release.artist_id %>
</p>

<% @release.tracks.each do |t| %>
<p>
	<%= link_to t.name, t.preview %>
</p>
<% end %>

<%= link_to 'Edit', edit_artist_release_path([@artist,@release]) %> |
<%= link_to 'Back', artist_releases_path %>
